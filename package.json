{
  "name": "alexa-skills",
  "version": "1.0.0",
  "main": "serverless.ts",
  "scripts": {
    "dev": "sls offline start",
    "config": "sls config credentials --provider aws --key key --secret secret",
    "lambda:deploy": "sls deploy --verbose",
    "alexa:auth": "sls alexa auth",
    "alexa:update": "sls alexa update --verbose",
    "start:one-piece-manga-spoiler": "sls invoke local -f OnePieceMangaSpoiler",
    "test": "jest --passWithNoTests --silent --noStackTrace --runInBand",
    "test:verbose": "jest --passWithNoTests --runInBand",
    "test:unit": "npm test -- --watch -c jest-unit-config.js",
    "test:integration": "npm test -- --watch -c jest-integration-config.js",
    "test:staged": "npm test -- --findRelatedTests",
    "test:ci": "npm test -- --coverage"
  },
  "engines": {
    "node": ">=14.15.0"
  },
  "dependencies": {
    "@middy/core": "^3.4.0",
    "@middy/http-json-body-parser": "^3.4.0",
    "ask-sdk": "^2.12.1",
    "ask-sdk-core": "^2.12.1",
    "ask-sdk-model": "^1.39.0",
    "aws-sdk": "^2.1240.0",
    "serverless-alexa-skills": "^0.1.0",
    "serverless-offline": "^11.1.3"
  },
  "devDependencies": {
    "@serverless/typescript": "^3.0.0",
    "@types/aws-lambda": "^8.10.71",
    "@types/jest": "^29.2.0",
    "@types/node": "^14.14.25",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "esbuild": "^0.14.11",
    "eslint": "^8.26.0",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-standard": "^5.0.0",
    "git-commit-msg-linter": "^4.2.1",
    "husky": "^8.0.1",
    "jest": "^29.2.2",
    "json-schema-to-ts": "^1.5.0",
    "lint-staged": "^13.0.3",
    "serverless": "^3.0.0",
    "serverless-esbuild": "^1.23.3",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.1.3"
  },
  "license": "MIT"
}
